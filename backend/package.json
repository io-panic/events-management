{
  "name": "featherjs-backend",
  "version": "1.0.0",
  "private": true,
  "main": "src",
  "keywords": [
    "feathers"
  ],
  "directories": {
    "lib": "src",
    "test": "test/",
    "config": "config/"
  },
  "engines": {
    "node": "^16.0.0",
    "npm": ">= 3.0.0"
  },
  "scripts": {
    "test": "npm run lint && npm run jest",
    "lint": "eslint src/. test/. --config .eslintrc.json --fix",
    "dev": "nodemon src/",
    "start": "node src/",
    "clean": "shx rm -rf test/data/",
    "jest": "npm run clean & SET NODE_ENV=test& jest --forceExit --detectOpenHandles"
  },
  "standard": {
    "env": [
      "jest"
    ],
    "ignore": []
  },
  "dependencies": {
    "@feathersjs/configuration": "^4.5.12",
    "@feathersjs/errors": "^4.5.12",
    "@feathersjs/express": "^4.5.12",
    "@feathersjs/feathers": "^4.5.12",
    "@feathersjs/transport-commons": "^4.5.12",
    "@seald-io/nedb": "^2.2.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "feathers-nedb": "^6.0.0",
    "helmet": "^4.6.0",
    "serve-favicon": "^2.5.0",
    "winston": "^3.6.0"
  },
  "devDependencies": {
    "axios": "^0.26.0",
    "eslint": "^8.10.0",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "shx": "^0.3.4"
  }
}
